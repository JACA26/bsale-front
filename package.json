{
  "name": "bsale-client",
  "version": "1.0.0",
  "description": "frontend for bsale",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --config ./webpack/webpack.config.js --env env=prod",
    "start": "webpack serve --config ./webpack.config.js --env env=dev --open",
    "clean": "rimraf dist",
    "babel": "babel entrada.js --out-file salida.js --presets=@babel/preset-env",
    "webpack": "webpack",
    "dev": "concurrently --kill-others \"npm run watch\" \"webpack-dev-server\"",
    "watch": "webpack --w"
  },
  "author": "Alexander",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@webpack-cli/serve": "^1.6.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "core-js": "^3.20.3",
    "regenerator-runtime": "^0.13.9"
  }
}
